import pymysql
 
con = pymysql.connect(host='localhost',
                    database='student_info',
                    user='root',
                    password='6256')
 
with con: 
 
    cur = con.cursor()
    cur.execute("SELECT * FROM student")
 
    rows = cur.fetchall()
    
    for row in rows:
        print("{0} {1} {2} {3} {4}".format(row[0], row[1], row[2], row[3], row[4]))
